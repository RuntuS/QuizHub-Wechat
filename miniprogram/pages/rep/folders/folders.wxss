/* miniprogram/pages/rep/folders/folders.wxss */
.to-somewhere{
  width: 100%;
  position: relative;
  height: 100rpx;
}


.to-somewhere > text{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 14%;
  font-size: 16px;
  font-weight: 400;
}


.to-somewhere > image{
  vertical-align: middle;
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);

}

.to-somewhere::after{
    content: "";
    position: absolute;
    width: 87%;
    height: 1px;
    border-top: 1px solid #BEBFBD;
    bottom: 0;
    right: 0;
}


#issue::after{
    content: "";
    position: absolute;
    width: 100%;
    height: 1px;
    border-top: 1px solid #BEBFBD;
    bottom: 0;
    right: 0;
}


.to-stars-icon{
  left: 5%;
}